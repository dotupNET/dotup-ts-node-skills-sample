{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "cleanup",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"clean-dist"
			]
		},
		{
			"label": "test file",
			"type": "shell",
			"command": "npm",
			"args": [
				"run",
				"test",
				"${file}"
			],
			"group": "test",
			"problemMatcher": "$jasmine"
		},
		{
			"type": "grunt",
			"task": "build",
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"type": "grunt",
			"task": "serve",
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"type": "grunt",
			"task": "run",
			"problemMatcher": []
		},
		{
			"type": "grunt",
			"task": "gh-pages",
			"problemMatcher": []
		},
		{
			"type": "grunt",
			"task": "release",
			"problemMatcher": [
				"$tsc"
			]
		},
		{
			"type": "grunt",
			"task": "lambda-deploy",
			"problemMatcher": [
				"$tsc"
			]
		}
	]
}